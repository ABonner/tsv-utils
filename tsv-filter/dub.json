{
    "name": "tsv-filter",
    "description": "Filter lines in a tab-separated value file.",
    "homepage": "https://github.com/eBay/tsv-utils-dlang",
    "authors": ["Jon Degenhardt"],
    "copyright": "Copyright (c) 2015-2016, eBay Software Foundation",
    "license": "BSL-1.0",
    "targetType": "executable",

    "configurations": [
        {
            "name" : "executable",
            "targetName": "tsv-filter",
            "targetPath": "../bin/",
            "mainSourceFile": "src/tsv-filter.d",
            "dependencies": { }
        },
        {
            "name": "unittest",
            "targetType": "none"
        }
    ],
    "buildTypes": {
        "debug": { "buildOptions": ["debugMode", "optimize"] },
        "release": { "buildOptions": ["releaseMode", "optimize"], "dflags": ["-boundscheck=off"] }
    }
}
